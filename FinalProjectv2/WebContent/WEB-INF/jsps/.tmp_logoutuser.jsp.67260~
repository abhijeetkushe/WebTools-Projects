<!-- start body -->
<script type="text/javascript">
      <!-- Comment out script for old browsers

       // Stores how many servers we need to sign out of
        var appCount = 0;

        // counter for how many servers replied (signed out / errored out)
        var runningCount = 0;

        // if true, one or more servers did not sign out successfully
        var errorOccurred = false;

        // holds the list of image objects, which when loaded, sign out of a server
        var loadedImages = [];

        // A logout image loaded successfully
        function imageLoadedHandler()
        {
         // mozilla seems to have trouble when redirection is too fast
          runningCount++;
          if (runningCount >= appCount)
             setTimeout(doRedirect, 2000);
        }

        // When an image cannot be loaded, note the error
        function imageErrorHandler()
        {
          runningCount++;
          errorOccurred = true;
          // mozilla seems to have trouble when redirection is too fast
          if (runningCount >= appCount)
              setTimeout(doRedirect, 2000);
        }

        // Redirect back to login page (or generic error page)
        function doRedirect()
        {
          var theForm = document.forms["logoutForm"];
          if (theForm != null)
          {
            if(errorOccurred)
            {
              alert('Logout error. Please close all your browser windows to complete the logout process.');
            }
          }
        }

        function submitOrRedirect()
        {
	   
		window.location.href = "https://myvodafone.vodafone.in/sso/pages/index.jsp";
	   
        }

   // -->
  </script>

           


