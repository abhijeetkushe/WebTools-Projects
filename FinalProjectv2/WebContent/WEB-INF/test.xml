<doctorList>
    <doctor>
        <firstName>
             #if($doctor.firstName !=null)
			 $doctor.firstName
        </firstName>
        <lastName>
            $doctor.lastName
        </lastName>
        <id>
            $doctor.id
        </id>
        <address>
            $doctor.addressLine1
        </address>
        <city>
            $doctor.city
        </city>
        <state>
            $doctor.state
        </state>
        <zipCode>
            $doctor.state
        </zipCode>
        <gender>
            $doctor.gender
        </gender>
        <dob>
            $dateTool.format("MM/dd/yyyy",$doctor.dateOfBirth)
        </dob>
        <age>
            $doctor.age
        </age>
        <ethnicity>
            $doctor.ethnicity
        </ethnicity>
        <title>
            Hardware Engineering Product Manager
        </title>
        <email>
            $doctor.email
        </email>
        <managerID>
            dtownson
        </managerID>
        <location>
            San Francisco
        </location>
        <homephone>
    		$doctor.homePhone
        </homephone>
        <cellphone>
            $doctor.cellPhone
        </cellphone>
        <maritalStatus>
            $doctor.maritalStatus
        </maritalStatus>
        <speciality>
            $doctor.speciality
        </speciality>
        <degree>
            $doctor.degree
        </degree>
        <ssn>
            $doctor.ssn
        </ssn>
        <officeAddress>
            $doctor.clinicAddress1
        </officeAddress>
        <officeCity>
            $doctor.clinicCity
        </officeCity>
        <officeState>
            $doctor.clinicState
        </officeState>
        <officeZip>
            $doctor.officeZipCode
        </officeZip>
        <officedays>
            $doctor.officeDays
        </officedays>
        <officeTimings>
            $doctor.officeHours
        </officeTimings>
        <professionalheadline>
            $doctor.professionalheadline
        </professionalheadline>
        <publicationList>
        	#foreach($publication in $doctor.publicationList)
            <publication>
            	<subject>
                    $publication.subject
                </subject>
				<hyperlink>
					$publication.hyperlink
				</hyperlink>	
                <description>
                    $publication.description
                </description>
            </publication>
			#end
        </publicationList>
        <reminderList>
        	#foreach($reminder in $doctor.reminderList)
        	<reminder> 
            	<description>
                	$reminder.description
            	</description>
            	<location>
               	 	$reminder.location
            	</location>
            	<time>
                	$reminder.time
          	  	</time>
			</reminder>
			#end
        </reminderList>
        <orderList>
        	#foreach($order in $doctor.orderList)
            <order>
                <ordertype>
                    $order.ordertype
                </ordertype>
                <patientName>
                    $order.patientName
                </patientName>
                <insurance>
                    $order.insurance
                </insurance>
                <desc>
                    $order.desc
                </desc>
                <date>
                    $dateTool.format("MM/dd/yyyy",$order.date)
                </date>
            </order>            
        </orderList>
        <image>
            $doctor.image
        </image>
    </doctor>
</doctorList>
